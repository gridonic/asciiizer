{
  "name": "@gridonic/asciiizer",
  "version": "1.0.0",
  "description": "A sample Vue.js app that generate ASCII art.",
  "author": "Gridonic <hello@gridonic.ch>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "test": "npm run build -- > /dev/null && echo ✔︎  OK",
    "start": "npm run build:prod && npm run serve",
    "serve": "http-server ."
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babili-webpack-plugin": "^0.1.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "faker": "^4.1.0",
    "figlet": "^1.2.0",
    "file-loader": "^0.11.1",
    "flexboxgrid-sass": "https://github.com/gridonic/flexboxgrid-sass/tarball/namespaced-breakpoints-variable",
    "http-server": "^0.10.0",
    "include-media": "^1.4.9",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "sass-svg-uri": "^1.0.0",
    "url-loader": "^0.5.7",
    "vue": "^2.3.3",
    "vue-loader": "^12.2.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "private": true,
  "engines": {
    "node": "^6"
  }
}
